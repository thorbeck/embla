{
  "name": "@abytegray/embla",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "./dist/web-components/index.cjs.js",
  "module": "./dist/web-components/index.js",
  "types": "./dist/web-components/types/components.d.ts",
  "collection": "./dist/web-components/collection/collection-manifest.json",
  "collection:main": "./dist/web-components/collection/index.js",
  "unpkg": "./dist/web-components/embla/embla.esm.js",
  "files": [
    "./dist/web-components/"
  ],
  "scripts": {
    "dev": "webpack --env development --watch",
    "dev:stencil": "stencil build --watch",
    "build": "webpack --env production",
    "build:stencil": "stencil build --docs-json ./dist/web-components/docs.json",
    "generate": "stencil generate",
    "deploy": "node ./gh-pages.js",
    "linked": "npm ls -g --depth=0 --link=true"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@stencil/postcss": "^2.1.0",
    "@stencil/sass": "^1.5.2",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "html-loader": "^3.1.0",
    "mini-css-extract-plugin": "^2.5.0",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "postcss-scss": "^4.0.3",
    "sass": "^1.47.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@stencil/core": "^2.12.1"
  },
  "license": "MIT"
}
